{
  "name": "@wbpesmnt/stitches-babel",
  "version": "0.0.0",
  "source": "./index.ts",
  "exports": {
    ".": {
      "import": "./lib/config.mjs",
      "require": "./lib/config.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./lib/index.d.ts",
  "main": "./lib/config.js",
  "module": "./lib/config.mjs",
  "engines": {
    "node": ">=14.0.0"
  },
  "license": "MIT",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "yarn rimraf ./lib && echo '  ðŸ§¹ Cleaned'",
    "build": "yarn clean && concurrently \"yarn:build:*\"",
    "build:dts": "tsc --emitDeclarationOnly --skipLibCheck",
    "build:cjs": "BABEL_ENV=commonjs babel src -d lib -x .ts --out-file-extension .js --source-maps",
    "build:esm": "BABEL_ENV=esmodule babel src -d lib -x .ts --out-file-extension .mjs --source-maps",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@stitches/react": "^1.2.6",
    "@wbpesmnt/design-token": "workspace:*"
  },
  "peerDependencies": {
    "@stitches/react": ">=1.0.0",
    "react": "^16.8 || ^17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-modules": "^0.1.5",
    "@babel/preset-typescript": "^7.16.7",
    "@wbpesmnt/tsconfig": "workspace:*",
    "babel-plugin-polished": "^1.1.0",
    "concurrently": "^7.0.0",
    "polished": "^4.1.3",
    "rimraf": "*",
    "typescript": "*"
  }
}
